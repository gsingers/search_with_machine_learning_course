{
  "featureset": {
    "features": [
      {
        "name": "fuzzy_name_match",
        "params": [
          "keywords"
        ],
        "template_language": "mustache",
        "template": {
          "match": {
            "name": {
              "query": "{{keywords}}",
              "fuzziness": "1",
              "boost": 0.01
            }
          }
        }
      },
      {
        "name": "name_phrase_match",
        "params": [
          "keywords"
        ],
        "template_language": "mustache",
        "template": {
          "match_phrase": {
            "name": {
              "query": "{{keywords}}",
              "slop": 1,
              "boost": 50
            }
          }
        }
      },
      {
        "name": "multi_match",
        "params": [
          "keywords"
        ],
        "template_language": "mustache",
        "template": {
          "multi_match": {
            "query": "{{keywords}}",
            "type": "phrase",
            "slop": "6",
            "minimum_should_match": "2<75%",
            "fields": [
              "name^10",
              "name.hyphens",
              "shortDescription",
              "longDescription",
              "department",
              "sku",
              "manufacturer",
              "features",
              "categoryPath"
            ]
          }
        }
      },
      {
        "name": "name_match",
        "params": [
          "keywords"
        ],
        "template_language": "mustache",
        "template": {
          "match": {
            "name": "{{keywords}}"
          }
        }
      },
      {
        "name": "name_hyphens_match",
        "params": [
          "keywords"
        ],
        "template_language": "mustache",
        "template": {
          "match": {
            "name.hyphens": "{{keywords}}"
          }
        }
      },
      {
        "name": "salesRankShortTerm",
        "template_language": "mustache",
        "template": {
          "function_score": {
            "functions": [
              {
                "field_value_factor": {
                  "field": "salesRankShortTerm",
                  "missing": 1000000
                }
              }
            ],
            "query": {
              "match_all": {}
            }
          }
        }
      },
      {
        "name": "salesRankMediumTerm",
        "template_language": "mustache",
        "template": {
          "function_score": {
            "functions": [
              {
                "field_value_factor": {
                  "field": "salesRankMediumTerm",
                  "missing": 1000000
                }
              }
            ],
            "query": {
              "match_all": {}
            }
          }
        }
      },
      {
        "name": "salesRankLongTerm",
        "template_language": "mustache",
        "template": {
          "function_score": {
            "functions": [
              {
                "field_value_factor": {
                  "field": "salesRankLongTerm",
                  "missing": 1000000
                }
              }
            ],
            "query": {
              "match_all": {}
            }
          }
        }
      },
      {
        "name": "shortDescription_match",
        "params": [
          "keywords"
        ],
        "template_language": "mustache",
        "template": {
          "match": {
            "shortDescription": "{{keywords}}"
          }
        }
      },
      {
        "name": "longDescription_match",
        "params": [
          "keywords"
        ],
        "template_language": "mustache",
        "template": {
          "match": {
            "longDescription": "{{keywords}}"
          }
        }
      },
      {
        "name": "name_hyphens_and_match",
        "params": [
          "keywords"
        ],
        "template_language": "mustache",
        "template": {
          "match": {
            "name.hyphens": {
              "query": "{{keywords}}",
              "operator": "OR",
              "minimum_should_match": "2<75%"
            }
          }
        }
      },
      {
        "name": "regularPrice",
        "template_language": "mustache",
        "template": {
          "function_score": {
            "functions": [
              {
                "field_value_factor": {
                  "field": "regularPrice",
                  "missing": 0
                }
              }
            ],
            "query": {
              "match_all": {}
            }
          }
        }
      },
      {
        "name": "salePrice",
        "template_language": "mustache",
        "template": {
          "function_score": {
            "functions": [
              {
                "field_value_factor": {
                  "field": "salePrice",
                  "missing": 0
                }
              }
            ],
            "query": {
              "match_all": {}
            }
          }
        }
      },
      {
        "name": "click_prior",
        "template_language": "mustache",
        "template": {
          "query_string": {
            "query": "{{click_prior_query}}",
            "fields": [
              "_id"
            ]
          }
        }
      }
    ]
  }
}