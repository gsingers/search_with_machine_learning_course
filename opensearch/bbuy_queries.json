{
  "settings": {
    "index.refresh_interval": "5s"
  },
  "mappings" : {
    "properties" : {
      "@timestamp" : {
        "type" : "date"
      },
      "@version" : {
        "type" : "long"
      },
      "category" : {
        "type" : "keyword",
        "ignore_above" : 512
      },
      "click_time" : {
        "type" : "date",
        "format": "yyyy-MM-dd HH:mm:ss.SSS||ordinal_date_time||strict_date_optional_time||epoch_millis"
      },
      "query" : {
        "type" : "text",
        "fields" : {
          "keyword" : {
            "type" : "keyword",
            "ignore_above" : 2048
          }
        }
      },
      "query_time" : {
        "type" : "date",
        "format": "yyyy-MM-dd HH:mm:ss.SSS||ordinal_date_time||strict_date_optional_time||epoch_millis"
      },
      "sku" : {
        "type" : "long"
      },
      "user" : {
        "type" : "keyword",
        "ignore_above" : 1024
      }
    }
  }
}