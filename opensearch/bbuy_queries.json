{
  "settings": {
      "index": {
          "refresh_interval": "-1",
          "number_of_shards": "6",
          "number_of_replicas": "0",
          "analysis": {
              "filter": {
                  "english_minimal_stemmer": {
                      "type": "stemmer",
                      "name": "minimal_english"
                  },
                  "word_delimiter_filter": {
                      "preserve_original": "true",
                      "catenate_words": "true",
                      "catenate_all": "true",
                      "split_on_case_change": "true",
                      "type": "word_delimiter",
                      "catenate_numbers": "true",
                      "stem_english_possessive": "false"
                  }
              },
              "analyzer": {
                  "text_word_delimiter": {
                      "filter": [
                          "word_delimiter_filter",
                          "asciifolding",
                          "lowercase",
                          "english_minimal_stemmer"
                      ],
                      "language": "English",
                      "type": "custom",
                      "tokenizer": "standard"
                  }
              }
          }
      }
  },
  "mappings": {
      "properties": {
          "user": {
              "type": "keyword",
              "store": true
          },
          "sku": {
              "type": "keyword",
              "store": true
          },
          "cateorgy": {
              "type": "keyword",
              "store": true
          },
          "query": {
              "type": "text",
              "fields": {
                  "keyword": {
                      "type": "keyword",
                      "ignore_above": 256
                  }
              },
              "analyzer": "text_word_delimiter",
              "store": true
          },
          "click_time": {
              "type": "date",
              "format": "yyyy-MM-dd HH:mm:ss.SSS||yyyy-MM-dd HH:mm:ss.SS||yyyy-MM-dd HH:mm:ss.S||yyyy-MM-dd HH:mm:ss"
          },
          "query_time": {
              "type": "date",
              "format": "yyyy-MM-dd HH:mm:ss.SSS||yyyy-MM-dd HH:mm:ss.SS||yyyy-MM-dd HH:mm:ss.S||yyyy-MM-dd HH:mm:ss"
          }
      }

  }
}
